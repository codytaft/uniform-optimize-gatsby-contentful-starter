{
  "name": "@uniformdev/optimize-starter-contentful-gatsby",
  "version": "11.0.0",
  "eslintIgnore": [
    "../../packages/**/dist/**/*.js"
  ],
  "description": "Uniform Gatsby starter site for contentful",
  "private": true,
  "scripts": {
    "build": "run-s generate:intents clean build:gatsby",
    "develop": "run-s generate:intents develop:gatsby",
    "build:gatsby": "gatsby build",
    "develop:gatsby": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "generate:intents": "uniform optimize manifest download --output ./src/lib/intentManifest.json",
    "ci:build": "run-s generate:intents clean build:gatsby"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@analytics/google-analytics": "^0.5.3",
    "@analytics/segment": "^1.1.0",
    "@n8tb1t/use-scroll-position": "^2.0.3",
    "@uniformdev/optimize-tracker-analytics": "^11.0.0",
    "@uniformdev/optimize-tracker-browser": "^11.0.0",
    "@uniformdev/optimize-tracker-contentful": "^11.0.0",
    "@uniformdev/optimize-tracker-react": "^11.0.0",
    "@uniformdev/optimize-tracker-storage-indexeddb": "^11.0.0",
    "analytics": "^0.7.13",
    "dotenv": "^10.0.0",
    "gatsby": "3.12.0",
    "gatsby-plugin-image": "^1.12.0",
    "gatsby-plugin-postcss": "^4.12.0",
    "gatsby-plugin-react-helmet": "^4.12.0",
    "gatsby-plugin-sharp": "^3.12.0",
    "gatsby-source-contentful": "^5.12.0",
    "js-cookie": "^3.0.0",
    "postcss": "^8.3.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@uniformdev/cli": "^11.0.0",
    "gatsby-cli": "3.12.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.3.2",
    "tailwindcss": "^2.2.7"
  }
}
